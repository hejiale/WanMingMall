.couponView {
  display: flex;
  align-items: center;
  margin: 10px;
  width: calc(100% - 20px);
  height: 111px;
  border-radius: 12rpx;
}

.leftCornerImage {
  width: 6px;
  height: 92px;
}

.priceView {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100px;
  height: 100%;
}

.priceTextTitle {
  margin-top: 10px;
  color: white;
  font-size: 20px;
  font-weight: 500;
}

.priceText {
  color: white;
  font-size: 45px;
  font-weight: 500;
}

.middleCornerView {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 11px;
  height: 100%;
}

.topCornerImage, .bottomCornerImage {
  width: 11px;
  height: 6px;
}

.bottomCornerImage {
  width: 11px;
  height: 6px;
}

.middleCornerImage {
  width: 3px;
  height: 99px;
}

.couponTitleView {
  display: flex;
  flex-direction: column;
  margin-left: 10px;
  width: calc(100% - 157px);
}

.couponTitle {
  color: white;
  font-size: 16px;
  font-weight: 500;
}

.couponMemo, .couponDate {
  color: white;
  font-size: 12px;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-weight: 300;
  overflow: hidden;
  padding-top: 5px;
}

.chooseImage {
  margin-top: 81px;
  width: 30px;
  height: 30px;
}